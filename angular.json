{
  "version": 1,
  "projects": {
    "ng-swag-action-tester": {
      "projectType": "application",
      "schematics": {},
      "root": "apps/ng-swag-action-tester",
      "sourceRoot": "apps/ng-swag-action-tester/src",
      "prefix": "ng-swag-action-tester",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/ng-swag-action-tester",
            "index": "apps/ng-swag-action-tester/src/index.html",
            "main": "apps/ng-swag-action-tester/src/main.ts",
            "polyfills": "apps/ng-swag-action-tester/src/polyfills.ts",
            "tsConfig": "apps/ng-swag-action-tester/tsconfig.app.json",
            "aot": false,
            "assets": [
              "apps/ng-swag-action-tester/src/favicon.ico",
              "apps/ng-swag-action-tester/src/assets"
            ],
            "styles": ["apps/ng-swag-action-tester/src/styles.css"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/ng-swag-action-tester/src/environments/environment.ts",
                  "with": "apps/ng-swag-action-tester/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "ng-swag-action-tester:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "ng-swag-action-tester:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ng-swag-action-tester:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/ng-swag-action-tester/tsconfig.app.json",
              "apps/ng-swag-action-tester/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/ng-swag-action-tester/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/ng-swag-action-tester/jest.config.js",
            "tsConfig": "apps/ng-swag-action-tester/tsconfig.spec.json",
            "setupFile": "apps/ng-swag-action-tester/src/test-setup.ts"
          }
        }
      }
    },
    "ng-swag-action-tester-e2e": {
      "root": "apps/ng-swag-action-tester-e2e",
      "sourceRoot": "apps/ng-swag-action-tester-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/ng-swag-action-tester-e2e/cypress.json",
            "tsConfig": "apps/ng-swag-action-tester-e2e/tsconfig.e2e.json",
            "devServerTarget": "ng-swag-action-tester:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "ng-swag-action-tester:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/ng-swag-action-tester-e2e/tsconfig.e2e.json"],
            "exclude": [
              "**/node_modules/**",
              "!apps/ng-swag-action-tester-e2e/**"
            ]
          }
        }
      }
    },
    "basic": {
      "root": "libs/client/basic",
      "sourceRoot": "libs/client/basic/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/client/basic/tsconfig.lib.json",
              "libs/client/basic/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/client/basic/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/client/basic/jest.config.js",
            "tsConfig": "libs/client/basic/tsconfig.spec.json"
          }
        }
      }
    },
    "swag-server": {
      "root": "apps/swag-server",
      "sourceRoot": "apps/swag-server/src",
      "projectType": "application",
      "prefix": "swag-server",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/swag-server",
            "main": "apps/swag-server/src/main.ts",
            "tsConfig": "apps/swag-server/tsconfig.app.json",
            "assets": ["apps/swag-server/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/swag-server/src/environments/environment.ts",
                  "with": "apps/swag-server/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "swag-server:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/swag-server/tsconfig.app.json",
              "apps/swag-server/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/swag-server/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/swag-server/jest.config.js",
            "tsConfig": "apps/swag-server/tsconfig.spec.json"
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    }
  },
  "defaultProject": "swag-server"
}
